  <select id="findDicLikeCode" resultMap="BaseResultMap" parameterType="com.e9cloud.mybatis.domain.DicData">
    select
    <include refid="Base_Column_List" />
    from tb_dic_data
    where state = 'Y'
    <if test="code != null and code != ''">
      AND code like "%"#{code,jdbcType=VARCHAR}"%"
    </if>
    <if test="typekey != null and typekey != ''">
      AND typekey = #{typekey,jdbcType=VARCHAR}
    </if>
    ORDER BY sort ASC
