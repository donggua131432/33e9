  <select id="findDicListByType" resultMap="BaseResultMap" parameterType="String">
      select id,name,code from tb_dic_data   where
      typekey = #{typekey,jdbcType=VARCHAR} and state = 'Y' order by sort asc
  </select>

  <select id="findDicListByPid" resultMap="BaseResultMap" parameterType="String">
       select id,name,code from tb_dic_data   where
       pid = #{pid,jdbcType=VARCHAR} and state = 'Y' order by sort asc
  </select>

  <select id="findDicByCode" resultMap="BaseResultMap" parameterType="com.e9cloud.mybatis.domain.DicData">
    select
    <include refid="Base_Column_List" />
    from tb_dic_data
    where state = 'Y'
